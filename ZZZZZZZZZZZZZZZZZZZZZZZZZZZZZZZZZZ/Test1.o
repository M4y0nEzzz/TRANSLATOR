MODULE Primes;
IMPORT In, Out;
CONST
    a = 1 + 34567;
VAR
    n, p, i, d, c, cnt: INTEGER;
BEGIN
    In.Open();
    In.Int(n);
    p := 2;
    cnt := 0;
    WHILE p <= n DO
        d := 2;
        c := p;
        WHILE d * d <= c DO
            IF c MOD d = 0 THEN
                c := 0;
            END;
            INC(d);
        END;
        IF c # 0 THEN
            Out.Int(c, 8);
            INC(cnt);
        END;
        INC(p);
    END;
    Out.Ln();
    Out.Int(cnt, 0);
    Out.Int(MAX(INTEGER), 0);
    Out.Int(MIN(INTEGER), 0);
    IF ODD(cnt) THEN
        Out.Int(1, 0);
    ELSE
        Out.Int(0, 0);
    END;
    HALT(cnt);
    Out.Int(4, 0);
END Primes.