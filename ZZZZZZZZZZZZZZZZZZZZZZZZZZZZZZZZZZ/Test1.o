MODULE Primes;
IMPORT In, Out;
VAR
    q, p, i, d, c, cnt: INTEGER;
BEGIN

    In.Open();
    In.Int(q);
    p := 2;
    cnt := 0;
    WHILE p <= q DO
        d := 2;
        c := p;
        WHILE d * d <= c DO
            IF c MOD d = 0 THEN
                c := 0;
            END;
            INC(d);
        END;
        IF c # 0 THEN
            Out.Int(c, 8);
            INC(cnt);
        END;
        INC(p); (*  inc(x)   x = x + 1  *)
    END;
    Out.Ln();
    Out.Int(cnt, 0);
    (*
    Out.Int(5 DIV 2, 0);
    *)
END Primes.